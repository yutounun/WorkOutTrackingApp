{
  "name": "workouttrackingapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "local": "vite --mode local-env",
    "dev": "vite --mode development-env",
    "prod": "vite --mode production-env",
    "build:local": "vite build --mode local-env",
    "build:dev": "vite build --mode development-env",
    "build:prod": "vite build  --mode production-env",
    "preview:local": "vite preview --mode local-env",
    "preview:dev": "vite preview --mode development-env",
    "preview:prod": "vite preview  --mode production-env",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:report": "npx playwright show-report",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "daisyui": "^2.49.0",
    "firebase": "^9.16.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@playwright/test": "^1.28.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.16",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^29.4.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.3",
    "@vitest/ui": "^0.28.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "happy-dom": "^8.2.2",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-pwa": "^0.14.1",
    "vitest": "^0.25.6",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.12"
  }
}
